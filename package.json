{
  "name": "wwt-data-bus",
  "version": "0.1.0",
  "private": true,
  "standard": "ignore",
  "author": "Dennis-Nedry-From-Jurassic-Park <dennis.nedry.from.jurassic.park@gmail.com>",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "source": "./index.ts",
  "types": "./index.ts",
  "workspaces": [
    "shared/**",
    "src/**"
  ],
  "scripts": {
    "-------- arkit -------": "",
    "arkit": "npx arkit src/ -o arkit.svg",
    "-------- git submodule -------": "",
    "git submodule add lib-base": "git submodule add --force https://github.com/Dennis-Nedry-From-Jurassic-Park/lib-base.git shared/lib-base",
    "git submodule add lib-db-client": "git submodule add --force https://github.com/Dennis-Nedry-From-Jurassic-Park/lib-db-client.git shared/lib-db-client",
    "git submodule add lib-msg-queue": "git submodule add --force https://github.com/Dennis-Nedry-From-Jurassic-Park/lib-msg-queue.git shared/lib-msg-queue",
    "git submodule update": "git submodule update --init --recursive && git submodule foreach --recursive git pull origin main",
    "git submodule sync": "git submodule sync",
    "add_deps": "",
    "@lib/db-client": "workspace:^",
    "pnpm:install:*": "npx pnpm -r install --recursive",
    "nest:build:*": "nest build",
    "OR": "",
    "pnpm:recursive:build": "pnpm run --parallel -r build",
    "-------- pnpm common ------------": "",
    "pnpm:add": "npx pnpm add -g pnpm",
    "pnpm:setup": "npx pnpm setup",
    "-------- pnpm package ----------": "",
    "pnpm:add:p": "npx pnpm add -w decimal.js @ankr.com/ankr.js",
    "pnpm:add:p:dev": "npx pnpm add -w -D benchmark-node",
    "pnpm:update-deps": "pnpm update --latest",
    "pnpm:install": "npx pnpm -r install --recursive",
    "pnpm:update-deps:for:packages": "pnpm update --filter packages --latest",
    "pnpm:list": "pnpm list -r --depth=-1",
    "----------- docker -----------": "",
    "docker:run:stop:all": "docker stop $(docker ps -a -q)",
    "docker:run:ms:app": "docker-compose -f docker-compose.yml up -d",
    "docker:down:ms:cron": "docker-compose -f docker-compose-cron.yml down --remove-orphans",
    "docker:tag:ms:cron": "docker tag 7097bc9a6667f62ef8cec701a78e785a1a8c059fd4ebe0c5c4b3ebbcb6761797 deps:0.0.1",
    "docker:build:ms:cron": "docker build . --no-cache --progress=tty -f docker/ms-cron/Dependencies.Dockerfile",
    "docker:run:ms:cron": "docker-compose -f docker-compose-cron.yml up -d --quiet-pull --build atr_ms_template",
    "----------- doppler -----------": "",
    "doppler:login": "doppler login",
    "doppler:setup": "doppler setup",
    "----------- runners -----------": "",
    "exec:doppler": "doppler run -- ts-node src/exec.ts",
    "uniswap.v2": "doppler run -- ts-node src/bot/trash/uniswap.v2.ts",
    "1inch.v5": "doppler run -- ts-node src/bot/trash/1inch.v5.ts",
    "blast": "doppler run -- ts-node src/bot/trash/blast.ts",
    "wwt": "doppler run -- ts-node src/wwt.ts",
    "wwt.exec": "doppler run -- ts-node src/wwt.exec.ts",
    "main": "doppler run -- ts-node src/bot/main.ts",
    "wwt.starknet": "doppler run -- ts-node src/wwt.starknet.ts",
    "wwt.portfolio.perf.stats": "doppler run -- ts-node src/wwt.portfolio.perf.stats.ts",
    "exec": "ts-node src/exec.ts",
    "try.ts": "ts-node src/try.ts",
    "bench": "ts-node bench/flatten.obj.ts",
    "build": "nest build",
    "start": "nest start",
    "rimraf:dist": "rimraf dist/* --glob",
    "rimraf:node_modules": "rimraf node_modules/* --glob",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint --color --fix --ext .ts,.js,.jsx packages/backend/apps/ms-src/src",
    "lint-q": "eslint --color --fix --quiet --ext .ts,.js,.jsx packages/backend/apps/ms-src/src",
    "test": "jest",
    "----------- nestjs -----------": "",
    "nestjs:init": "npx nest g resource"
  },
  "dependencies": {
    "@1inch/fusion-sdk": "^2.1.1",
    "@1inch/limit-order-protocol-utils": "4.0.1",
    "@ankr.com/ankr.js": "^0.5.0",
    "@bwarelabs/blast-sdk-js": "^1.0.17",
    "@golevelup/nestjs-rabbitmq": "^5.3.0",
    "@magna_shogun/catch-decorator": "^1.0.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-fastify": "^10.3.8",
    "abitype": "^1.0.2",
    "amqplib": "^0.10.4",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "ethers": "^6.12.1",
    "flat": "^6.0.1",
    "flat-obj": "^3.0.0",
    "rimraf": "^5.0.7",
    "run-parallel": "^1.2.0",
    "rxjs": "^7.8.1",
    "solc": "^0.8.25",
    "starknet": "^5.24.3",
    "tslib": "^2.6.2",
    "typechain": "^8.3.2",
    "uuid": "^9.0.1",
    "viem": "^2.10.9",
    "web3": "^4.8.0"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/flat": "^5.0.5",
    "@types/jest": "29.5.12",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "arkit": "^1.6.4",
    "benchmark-node": "^0.7.3",
    "jest": "^29.7.0",
    "jest-ts-auto-mock": "^2.1.0",
    "object-sizeof": "^2.6.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.6.7"
}
